{
  "name": "flowbite-vue",
  "version": "0.0.2",
  "repository": "https://github.com/themesberg/flowbite-vue.git",
  "author": "themesberg",
  "license": "MIT",
  "files": [
    "dist",
    "dist_types"
  ],
  "main": "./dist/flowbite-vue.umd.js",
  "module": "./dist/flowbite-vue.es.js",
  "typings": "./dist_types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/flowbite-vue.es.js",
      "require": "./dist/flowbite-vue.umd.js"
    }
  },
  "scripts": {
    "dev": "vitepress dev docs",
    "start": "vitepress serve docs --port $PORT",
    "build": "vitepress build docs && cp -r ./docs/assets/* ./docs/.vitepress/dist/assets/",
    "build:package": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "build:production": "npm run build:types && npm run build:package",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3",
    "tailwindcss": "^3"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.11.3",
    "class-names": "^1.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^9.1.1",
    "flowbite": "^1.4.2",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.14",
    "postcss-prefix-selector": "^1.16.0",
    "prettier": "^2.3.2",
    "tailwindcss": "^3",
    "typescript": "^4.7.3",
    "vite": "^2.4.3",
    "vitest": "^0.16.0",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.30.0"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.1",
    "http-server": "^14.1.1",
    "vitepress": "^1.0.0-alpha.4"
  }
}
