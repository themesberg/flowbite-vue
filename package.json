{
  "name": "flowbite-vue",
  "version": "0.1.7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/themesberg/flowbite-vue.git"
  },
  "author": "themesberg",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/flowbite-vue.umd.js",
  "module": "./dist/flowbite-vue.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/flowbite-vue.mjs",
      "require": "./dist/flowbite-vue.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "dev": "vitepress dev docs",
    "start": "vitepress serve docs --port $PORT",
    "build": "vitepress build docs && cp -r ./docs/assets/* ./docs/.vitepress/dist/assets/",
    "build:package": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly && tsc-alias -p tsconfig.json",
    "build:production": "npm run build:package && npm run build:types",
    "lint": "eslint . --ext .js,.ts,.vue --ignore-path .gitignore",
    "lint-fix": "eslint . --fix --ext .js,.ts,.vue --ignore-path .gitignore",
    "clear": "rm -fr ./dist && rm -fr ./dist_types",
    "format": "npm run lint-fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build:production"
  },
  "peerDependencies": {
    "tailwindcss": "^3",
    "vue": "^3.4.x"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.5",
    "@types/lodash-es": "4.17.12",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/coverage-v8": "1.6.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "8.57.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-vue": "9.32.0",
    "eslint-plugin-vue-scoped-css": "2.9.0",
    "jsdom": "24.1.3",
    "postcss": "8.5.1",
    "postcss-prefix-selector": "1.16.1",
    "tsc-alias": "1.8.10",
    "typescript": "5.7.3",
    "vite": "5.4.14",
    "vitepress": "1.6.1",
    "vitest": "1.6.0",
    "vue-tsc": "2.2.0"
  },
  "dependencies": {
    "@vueuse/core": "10.11.1",
    "classnames": "2.5.1",
    "floating-vue": "^5.2.2",
    "flowbite": "2.5.2",
    "lodash-es": "4.17.21",
    "nanoid": "5.0.9",
    "tailwind-merge": "2.6.0",
    "tailwindcss": "^3.4.17"
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=10.x"
  }
}
